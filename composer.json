{
    "name": "illuminatech/array-factory",
    "description": "Allows DI aware object creation from array definition",
    "keywords": ["laravel", "factory", "array", "configuration", "config"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/illuminatech/array-factory/issues",
        "wiki": "https://github.com/illuminatech/array-factory/wiki",
        "source": "https://github.com/illuminatech/array-factory"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "illuminate/container": "*",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.3 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Illuminatech\\ArrayFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminatech\\ArrayFactory\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Illuminatech\\ArrayFactory\\FactoryServiceProvider"
            ]
        }
    }
}